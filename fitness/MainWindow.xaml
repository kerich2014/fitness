<Window x:Class="fitness.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:fitness"
        mc:Ignorable="d"
        Title="Olimpia" Height="600" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="120"/>
            <RowDefinition Height="330"/>
        </Grid.RowDefinitions>
        <Frame x:Name="frmMain" Content="" Grid.Row="1" NavigationUIVisibility="Hidden" Margin="0,0,0,-124"/>
        <Image HorizontalAlignment="Left" Height="100" Margin="10,0,0,0" VerticalAlignment="Center" Width="148" Source="/logo.png"/>
        <Label Content="Olympia" HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Calibri Light" FontSize="48" FontWeight="Normal"/>
    </Grid>
</Window>
